<h1>Lista de Contatos</h1>

{{#each posts}}
<h2>{{dataValues.nome}}</h2>
    <h2>{{dataValues.numero}}</h2>
    <h2>{{dataValues.foto}}</h2>
<a href="/editar/{{dataValues.idContato}}"><button>Editar</button></a>
<a href="/deletar/{{dataValues.idContato}}"> <button>Deletar</button></a>
<a href="/fav/{{dataValues.idContato}}"><button>Adicionar Favoritos</button></a>
<a href="/bloq/{{dataValues.idContato}}"> <button>Bloquear</button></a>
{{/each}}
